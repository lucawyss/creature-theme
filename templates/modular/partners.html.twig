	<div class="row container">
	    <h4 class="header">{{ page.title }}</h4>
	    	{% for partner in page.header.partners %}
		        <article class="col s4 m3 l2">
		        	{% if (partner.url) and (partner.image) %}
		        	{% set logo = page.media[partner.image].resize(200, 200).grayscale().negate.brightness(150) %}
			        <a href="{{ partner.url }}" target="_blank">
	                	<img src="{{ logo.url }}" alt="{{ partner.header }}">
	                </a>
	                {% elseif (feature.image) and (not feature.url) %}
	                	<img src="{{ partner.image }}" alt="{{ partner.header }}">
	                {% endif %}
			    </article>
		    {% endfor %}
	</div>
	<div class="row container">
	    <h4 class="header">{{ page.title }}</h4>
	    <ul class="collapsible popout">
	    {% for paragraph in page.content|split('<hr />') %}
	    	{% set element = paragraph|split(':') %}
		    <li>
		      <h5 class="collapsible-header active">{{ element[0]}}</h5>
		      <div class="collapsible-body">
		      	<p>{{ element[1] }}</p>
		      </div>
		    </li>
		{% endfor %}
		</ul>
	</div>

            
            	
            